{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-api",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/ShopQualityboltWeb/ShopQualityboltWeb/ShopQualityboltWeb.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary;ForceNoAlign"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "build-blazor",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/ShopQualityboltWebBlazor/ShopQualityboltWebBlazor.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary;ForceNoAlign"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "watch-api",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "--project",
                "${workspaceFolder}/ShopQualityboltWeb/ShopQualityboltWeb/ShopQualityboltWeb.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch-blazor",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "--project",
                "${workspaceFolder}/ShopQualityboltWebBlazor/ShopQualityboltWebBlazor.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}
